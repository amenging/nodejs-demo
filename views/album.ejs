<%- include templates/header %>
		<div class="album">
			<div class="btn btn-primary col-sm-9" style="margin-bottom: 10px;">照片列表</div>
			<div class="btn btn-primary col-sm-1 col-sm-offset-1 addPdiv" style="margin-bottom: 10px;" id="t1" onclick="fulAvatar.click()"><i class="glyphicon glyphicon-plus"></i></div>
			<br>
			<form class="form-horizontal log_div" role="form" method="post" enctype='multipart/form-data'>
					<div class="form-group">
						<div class="col-sm-2">
							<input id="fulAvatar" type="file" class="form-control hide" name="fulAvatar" multiple>
						</div>
					</div>
					<div class="form-group">
						<div class="col-sm-10 hide img_pre">
							<div>图片预览</div>
							<div class="output" id="out"></div>
						</div>
						<div class="col-sm-2">
							<input id="btnSub" type="submit" class="hide upload form-control btn btn-info" value="确认上传" style="margin-top: 20px;">
						</div>
					</div>
				</form>
				<ul class="photos col-sm-12">
					<% for (var i = 0;i < path.length;i ++) { %>
					<li>
						<div><img src="<%= path[i] %>" class="photos img-thumbnail"></div>
					</li>
					<% } %>
					<!-- <li class="addphoto" id="t1" onclick="fulAvatar.click()">
						<div class="addPdiv">+</div>
					</li> -->
				</ul>
				<br>
				
				<% if (locals.success) { %> 
					<div id="alt_sucess" class="alert alert-success"> 
					<%- success %> 
					</div> 
				<% } %> 
				<% if (locals.error) { %> 
					<div id="alt_warning" class="alert alert-warning">
						<%= error %> 
					</div> 
				<% } %>
				<div class="view"></div>
				<div class='view_div'></div>
		</div>
	</div>
<%- include templates/footer %>