<%- include templates/header %>
	<style>#testactive1{background:#333}</style>
	<form class="form-horizontal log_div" role="form" method="post">
	   <div class="form-group">
	      <div class="col-sm-5">
	         <input type="text" class="form-control name" id="firstname" name="name" placeholder="昵称">
	      </div>
	   </div>
	   <div class="form-group">
	      <div class="col-sm-5">
	         <input type="password" class="form-control password" name="password" id="lastname" placeholder="密码">
	      </div>
	   </div>
	   <div class="form-group">
	      <div class="col-sm-5">
	         <button type="submit" class="btn btn-default log">登录</button>
	      </div>
	   </div>
	</form>
<%- message %>
<%- include templates/footer %>