<%- include templates/header %>


<div class="search_div">
	<div class="col-sm-8">
		<input type="search" class="form-control" placeholder="输入名称搜索">
	</div>
	<div class="col-sm-4">
		<a type="button" class="form-control btn-primary" href="/addarticle" style="text-align: center;text-decoration: none;">新建</a>
	</div>
</div>
<style type="text/css">
.search_div {
	/*width: */
}
</style>

<div style="margin-top: 1em" class="col-sm-12">
	<% vacs.forEach(function(vacs){ %>
	<div class="panel panel-default">
		<div class="panel-heading">
			<h3 class="panel-title"><%= vacs.title %></h3>
		</div>
		<div class="panel-body">
			<div style="color: #777;font-size: .9em;">作者:<%= vacs.name %> | 时间:<%= vacs.year %></div>
			<%= vacs.contant %>
		</div>
	</div>
	<% }); %>
</div>

<%- include templates/footer %>