<%- include templates/header %>
<div class="search_div">
	<div class="col-sm-8">
		<input type="search" class="form-control article_search" placeholder="输入名称搜索">
	</div>
	<div class="col-sm-4">
		<a type="button" class="form-control btn-primary" href="/addarticle" style="text-align: center;text-decoration: none;">新建</a>
	</div>
</div>
<div style="margin-top: 1em" class="col-sm-12 articles">
	<% vacs.forEach(function(vacs){ %>
	<div class="panel panel-default articles_div">
		<div class="panel-heading">
			<h3 class="panel-title titles"><%= vacs.title %></h3>
			<a href="/change?titles=<%= vacs.title %>&time=<%= vacs.year %>"><button class="btn btn-info pull-right" style="position: relative;top: -26px;margin-left: 5px;">修改</button></a>
			<a href="/del?titles=<%= vacs.title %>&time=<%= vacs.year %>"><button class="btn btn-info pull-right" style="position: relative;top: -26px;">删除</button></a>
		</div>
		<div class="panel-body">
			<div style="color: #777;font-size: .9em;">作者:<%= vacs.name %> | 时间:<%= vacs.year %></div>
			<%= vacs.contant %>
		</div>
	</div>
	<% }); %>
</div>

<%- include templates/footer %>