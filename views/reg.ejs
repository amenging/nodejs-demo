<%- include templates/header %>
	<form class="form-horizontal reg_div" role="form" method="post">
   <div class="form-group">
    <div class="col-sm-5">
      <input type="text" name="name" class="form-control name" id="firstname" placeholder="请输入名字">
    </div>
   </div>
   <div class="form-group">
    <div class="col-sm-5">
      <input type="password" class="form-control password" name="password" id="lastname" placeholder="请输入密码">
    </div>
   </div>
   <div class="form-group">
    <div class="col-sm-5">
      <input type="password" class="form-control repass" name="repass" id="lastname" placeholder="重复密码">
    </div>
   </div>
   <div class="form-group">
    <div class="col-sm-5">
    	<button type="submit" class="btn btn-default log">注册</button>
    </div>
   </div>
	</form>
<%- message %>
<%- css %>
